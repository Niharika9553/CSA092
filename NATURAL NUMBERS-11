#include<stdio.h>
int main()
{ 
    int num,sum=0;
    printf("enter the  number=");
    scanf("%d",&num);
    for(int i=1;i<=num;i++)
    {
        sum=sum+i;
    }
    printf("the sum is=%d",sum);
    return 0;
}
OUTPUT;
#include<stdio.h>
int main()
{ 
    int num,sum=0;
    printf("enter the  number=");
    scanf("%d",&num);
    for(int i=1;i<=num;i++)
    {
        sum=sum+i;
    }
    printf("the sum is=%d",sum);
    return 0;
}


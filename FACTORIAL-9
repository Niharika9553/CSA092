#include<stdio.h>
int main ()
{
    int i,fact=1,num;
    printf("enter any number=");
    scanf("%d",&num);
    for(i=1;i<=num;++i)
    { 
      fact=fact*i;
    }
    printf("factorial of %d is=%d",num,fact);
    return 0;
}
OUTPUT;
enter any number=5
factorial of 5 is=120

=== Code Execution Successful ===

#include<stdio.h>
int main()
{
    int year=2026;
    if(year%400==0)
    {
        printf("%d is a leap year",year);
    }
    else if(year%100==0)
    {
        printf("%d is not a leap year",year);
    }
    else if(year%4==0)
    {
        printf("%d is a leap year",year);
    }
    else{
        printf("%d is not a leap year",year);
    }
    return 0;
}
output;
2026 is not a leap year
